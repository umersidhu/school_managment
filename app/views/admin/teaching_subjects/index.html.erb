<div id="content-header">
  <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="fa fa-home"></i> Home</a> <a href="#" class="current">Teachers of <%= @class_section.branch_class.name %> (<%= @class_section.section %>)</a> </div>
  <div class="custom-header">
    <h1>Teachers 
      <span class="pull-right">
        <%= link_to "Assign Teachers", new_admin_branch_class_section_teaching_subject_path(@branch_class, @class_section), class: "btn btn-primary pull-right", remote: true%>
      </span>
    </h1>
  </div>
</div>
<div class="container-fluid">
  <hr>
  <div class="row-fluid">
    <div class="span12">
      <div class="widget-box">
        <div class="widget-title"> <span class="icon"> <i class="fa fa-th"></i> </span>
          <h5>All Teachers</h5>
        </div>
        <div class="widget-content nopadding" id= "section-teachers">
          <%= render partial: "teachers", locals: {teachers: @teachers, class_section: @class_section}%>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="teacher-modal">
</div>

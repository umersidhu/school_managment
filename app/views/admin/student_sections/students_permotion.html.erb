 <div id="content-header">
  <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="fa fa-home"></i> Home</a> <a href="#" class="current">Students of <%= @class_section.branch_class.name %> (<%= @class_section.section %>)</a> </div>
  <div class="custom-header">
    <h1>Students Permotion 
    </h1>
  </div>
</div>
<div class="container-fluid">
  <hr>
  <div class="row-fluid">
    <div class="span12">
      <div class="widget-box">
        <div class="widget-title"> <span class="icon"> <i class="fa fa-th"></i> </span>
          <h5>All Students</h5>
        </div>
        <div class="widget-content nopadding" id= "section-students">
          <table class="table table-striped table-bordered">
            <thead>
              <th>Full Name</th>
              <th>Action</th>
            </thead>
            <tbody>
              <% @class_section&.student_sections.uniq.each do |student_section|%>
                <tr>
                  <div id="permotion-actions">
                    <%= render partial: "permotion_actions", locals: {student_section: student_section}%>
                  </div>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
